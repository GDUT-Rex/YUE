/**index.wxss**/

page {
  background: #ffffff;
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
  justify-content: flex-start;
}
.textContent{
  /* position: absolute; */
  width: 100%;
  color: #dddddd;
  height: 200px;
  background: url('https://cloud-minapp-32998.cloud.ifanrusercontent.com/1j0dgKEgsQ5dYQ50.jpg');
  /* top:20px; */
}
.mapTitle{
  width: 100%;
  padding-left: 20px;
  display: flex;
  align-items: center;

}
.bottom{
  display: flex;
  align-items: center;
  border-bottom: 2px solid #dddddd;
  padding-bottom: 3px;
}
.mapTitleText{
  padding-right: 5px;
}
.circle{
  margin-right: 10px;
  width: 10px;
  height: 10px;
  border-radius: 5px;
  background: #06516E;
}
